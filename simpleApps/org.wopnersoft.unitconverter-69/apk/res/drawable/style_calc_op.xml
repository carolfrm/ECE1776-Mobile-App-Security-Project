<?xml version="1.0" encoding="utf-8"?>
<selector
  xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <gradient android:startColor="#ffffb516" android:endColor="#fff48e17" android:angle="270.0" />
            <stroke android:width="2.0dip" android:color="#ff686865" />
            <corners android:radius="2.0dip" />
        </shape>
    </item>
    <item android:state_focused="true">
        <shape>
            <gradient android:startColor="#ffd0e0cf" android:endColor="#ffd0e0cf" android:angle="270.0" />
            <stroke android:width="2.0dip" android:color="#ff686865" />
            <corners android:radius="2.0dip" />
        </shape>
    </item>
    <item android:state_enabled="false">
        <shape>
            <solid android:color="#ffd4d4d4" />
            <stroke android:width="2.0dip" android:color="#ff686865" />
            <corners android:radius="2.0dip" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:startColor="#ffdedede" android:endColor="#ffb1b1b1" android:angle="270.0" />
            <stroke android:width="2.0dip" android:color="#ff686865" />
            <corners android:radius="2.0dip" />
        </shape>
    </item>
</selector>